<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:state_checked="true"><shape>
            <solid android:color="#66FFFF00" />
        </shape></item>"

    <item android:state_enabled="true"><shape>
            <solid android:color="#66F00000" />
        </shape></item>
    <item android:state_enabled="false" android:state_focused="true"><shape>
            <solid android:color="#66F00000" />
        </shape></item>
    <item android:state_pressed="true"><shape>
            <solid android:color="#6600F000" />
        </shape></item>
    <item android:state_focused="true"><shape>
            <solid android:color="#660000F0" />
        </shape></item>
    <item android:state_selected="true"><shape>
            <solid android:color="#66F00000" />
        </shape></item>
    <item android:state_activated="true"><shape>
            <solid android:color="#66F00000" />
        </shape></item>
    <item><shape>
            <solid android:color="#6600F000" />
        </shape></item>

</selector>